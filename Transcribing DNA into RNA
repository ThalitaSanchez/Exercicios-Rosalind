def completRNA (dna):
    basecomplementDNA={'T':'U','G':'G','A':'A','C':'C'}
    letters= list(dna)
    letters = [basecomplementDNA[base]for base in letters]
    print(''.join(letters))
   

dna='GATGGAACTTGACTACGTAAATT'
completRNA(dna)
